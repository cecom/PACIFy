  pacify\pacify-full\bin\pacify.cmd writePropertyFile --resolvers=FileResolver -DFileResolver.file=properties\kiwi_pint.properties

writePropertyFile to file:
  pacify\pacify-full\bin\pacify.cmd writePropertyFile --resolvers=FileResolver -DFileResolver.file=properties\kiwi_pint.properties --destinationFile=result.txt

replace:
  pacify\pacify-full\bin\pacify.cmd replace --resolvers=FileResolver -DFileResolver.file=properties\kiwi_pint.properties --envName=pint --packagePath=KIWI\15.01.00.00\01.00\03_Server
